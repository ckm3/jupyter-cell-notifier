{
  "name": "jupyter-cell-notifier",
  "displayName": "Jupyter Cell Notifier",
  "description": "Get notifications when Jupyter notebook cells finish running",
  "publisher": "KaimingCui",
  "version": "0.0.1",
  "license": "MIT",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onNotebook:jupyter-notebook"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Jupyter Cell Notifier",
      "properties": {
        "jupyter-cell-notifier.systemNotifications": {
          "type": "boolean",
          "default": true,
          "description": "Also show a system notification (macOS Notification Center) when a cell finishes."
        }
      }
    },
    "commands": [
      {
        "command": "jupyter-cell-notifier.toggleNotification",
        "title": "Toggle Cell Notification",
        "icon": "$(bell)"
      }
    ],
    "menus": {
      "notebook/cell/title": [
        {
          "command": "jupyter-cell-notifier.toggleNotification",
          "when": "notebookType == jupyter-notebook",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.3"
  }
}